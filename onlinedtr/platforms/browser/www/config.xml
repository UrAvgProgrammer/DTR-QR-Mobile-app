<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.onlinedtr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>OnlineDTR</name>
    <description>
        Mobile app for Online DTR
    </description>
    <author email="jrldtlld@gmail.com" href="http://github.com/jrldtlld">
        Jerald Talledo
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
